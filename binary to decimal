#include <stdio.h>
#include <math.h>
int main()
{
 int binaryno,decimalno=0;
 int i=0;
 printf("enter the binary no:");
 scanf("%d",&binaryno);
 while (binaryno !=0)
 {
  decimalno += binaryno%10 * pow(2,i);
  binaryno /=10;
  i++;
  }
  printf("the decimal no of binary no is %d",decimalno);
  return 0;
}
