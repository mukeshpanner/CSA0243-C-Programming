#include<stdio.h>
#include<math.h>
int main()
{
 float a,b,c,s,area;
 printf("enter the side of triangle A:");
 scanf("%f",&a);
 printf("enter the side of triangle B:");
 scanf("%f",&b);
 printf("enter the side of triangle C:");
 scanf("%f",&c);

 s = (a+b+c)/2;

 area = sqrt((s)*(s-a)*(s-b)*(s-c));
 printf("area of triangle:%f",area);
 return 0;
}
